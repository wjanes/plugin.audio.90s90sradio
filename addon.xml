<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.audio.90s90s" name="90s90s Radio" version="1.0.0" provider-name="Groovie">
    <requires>
        <import addon="xbmc.python" version="2.20.0" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="de_DE">90s90s Radio - Hits from the 90s</summary>
        <license>GNU General Public License, v2</license>
        <description lang="de_DE">90s90s ist das Radioangebot für alle, die die Musik der 90er lieben - und im hier und heute neu entdecken wollen. 90s90s bietet Dir viele kostenlose 90er Radiokanäle.</description>
        <platform>all</platform>
        <assets>
            <icon>resources/images/logo.png</icon>
            <fanart>resources/images/background.jpg</fanart>
        </assets>
    </extension>
</addon>